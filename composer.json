{
    "name": "activewebsite/fancybox",
    "description": "fancyBox is a tool that offers a nice and elegant way to add zooming functionality for images, html content and multi-media on your webpages.",
    "repositories": [
        {
            "type": "composer",
            "url": "http://qa.stg.booj.com"
        }
    ],
    "require": {
        "components/jquery": "~1.10"
    },
    "extra": {
        "component": {
            "files": [
                "source/*.js",
                "source/*.gif",
                "source/*.png",
                "source/less/*.less",
                "source/less/helpers/*.less",
                "source/helpers/*.js"
            ]
        }
    }
}
